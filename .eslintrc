{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jquery": true,
    "jasmine": true,
    "jest": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/forbid-prop-types": [0],
    "import/extensions": [0],
    "import/no-extraneous-dependencies": [0],
    "import/no-unresolved": [0],
    "import/no-absolute-path": [0],
    "import/no-named-as-default": [0],
    "import/prefer-default-export": [0],
    "global-require": [0],
    "linebreak-style": [0],
    "no-extra-semi": 2,
    "no-new": [0],
    "no-console": 2,
    "no-debugger": 2,
    "no-extra-parens": 2,
    "eqeqeq": 2,
    "no-unused-vars": 2,
    "semi": 2,
    "semi-spacing": 2,
    "array-bracket-spacing": 2,
    "arrow-spacing": 2,
    "arrow-body-style": 0,
    "comma-style": 2,
    "comma-spacing": 2,
    "indent": [2, 2],
    "eol-last": 2,
    "max-len": [2, { "code": 120 }],
    "func-call-spacing": 2,
    "no-alert": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-array-constructor": 2,
    "no-unused-expressions": 2,
    "no-empty-function": 2,
    "no-mixed-operators": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-inline-comments": 1,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "function-paren-newline": 0,
    "no-nested-ternary": 0,
    "jsx-a11y/anchor-is-valid": 0
  }
}